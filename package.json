  "scripts": {
    "build": "rimraf -rf client/production && set NODE_ENV=production& webpack --progress --p --display-error-details",
    "lint": "eslint .",
    "migrate:make": "babel-node --presets stage-2 node_modules/knex/bin/cli.js -- migrate:make",
    "migrate": "babel-node --presets stage-2 node_modules/knex/bin/cli.js -- migrate:latest",
    "seed:make": "babel-node --presets stage-2 node_modules/knex/bin/cli.js -- seed:make",
    "seed": "set NODE_ENV=test& babel-node --presets stage-2 node_modules/knex/bin/cli.js -- seed:run",
    "start:webpack": "webpack-dev-server --inline --progress --profile --colors",
    "start:dev": "set NODE_ENV=development& nodemon",
    "start:prod": "set NODE_ENV=production& babel-node src/server.js --presets react",
    "start:test": "set NODE_ENV=test& nodemon",
    "start": "npm run start:prod",
    "test": "npm run jest && npm run unit && npm run integration",
    "integration": "npm run seed && NODE_ENV=test mocha --compilers js:babel-core/register --require src/db.js src/tests/integration/index.js --timeout 50000",
    "unit": "set NODE_ENV=test& mocha --compilers js:babel-core/register --require src/db.js src/tests/unit/index.js --timeout 50000",
    "jest": "jest",
    "jest:watch": "npm jest -- --watch",
    "heroku-postbuild": "npm run migrate && npm run build"
  },
  "keywords": [
    "holipay"
  ],
